{
  "name": "Geekie API Explorer",
  "description": "Chrome App for easily interacting with Geekie APIs",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": { "32": "logo_32.png", "128": "logo_128.jpg" },
  "permissions": [
    "storage",
    "cookies",
    "http://*/*", 
    "https://*/*"
  ]
}
