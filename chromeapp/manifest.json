{
  "name": "Geekie API Explorer",
  "description": "Chrome App for easily interacting with Geekie APIs",
  "version": "0.1.1.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["launcher.js"]
    }
  },
  "icons": { 
    "32": "logo_32.png", 
    "128": "logo_128.png", 
    "16": "logo_16.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "cookies",
    "http://*/*", 
    "https://*/*"
  ]
}
